<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fiuady.android.compustorevv10.MissingProductsActivity">

    <Spinner
        android:id="@+id/spinner_orders"
        android:layout_width="226dp"
        android:layout_height="18dp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/toolbar2"
        android:layout_toEndOf="@+id/textView4"
        android:layout_marginStart="15dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/spinner_orders"
        android:layout_alignParentStart="true"
        android:layout_marginRight="289dp"
        android:layout_marginStart="31dp"
        android:text="# de Orden: "
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="50dp" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:title="Productos Faltantes" />
</RelativeLayout>
